{
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "import/no-unresolved":0,
      "no-use-before-define": 0,
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }]
    }
}
